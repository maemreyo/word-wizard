{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules",
    "api-proxy"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "jsx": "react",
    "paths": {
      "~*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "baseUrl": "."
  }
}